# Cloudinary Configuration
NEXT_PUBLIC_CLOUDINARY_CLOUD_NAME=your-cloud-name
NEXT_PUBLIC_CLOUDINARY_UPLOAD_PRESET=your-upload-preset
CLOUDINARY_URL=your-cloudinary-url
CLOUDINARY_API_KEY=your-api-key
CLOUDINARY_API_SECRET=your-api-secret
FACEBOOK_ACCESS_TOKEN=your-facebook-token

# Stripe Configuration
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=your-publishable-key
STRIPE_SECRET_KEY=your-secret-key
# Webhook secret from Stripe Dashboard > Developers > Webhooks
# Endpoint should be: https://fabrowx-website.vercel.app/api/stripe/webhook/
STRIPE_WEBHOOK_SECRET=your-webhook-secret

# Xero OAuth2 Configuration
XERO_CLIENT_ID=your-client-id
XERO_CLIENT_SECRET=your-client-secret
XERO_REDIRECT_URI=https://fabrowx-website.vercel.app/api/xero/callback
# These values are used by the webhook handler
XERO_TENANT_ID=your-tenant-id
XERO_ACCESS_TOKEN=your-access-token
XERO_REFRESH_TOKEN=your-refresh-token
XERO_EXPIRES_AT=your-expiry-timestamp
# Xero invoice theme IDs
XERO_ONLINE_THEME_ID=your-online-theme-id
XERO_WORKSHOP_THEME_ID=your-workshop-theme-id 